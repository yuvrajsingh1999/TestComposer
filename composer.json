{
    "name": "testYuvi/yuvraj",
    "description": "Test to Host composer package for laravel",
    "keywords": [
        "testYuvi",
        "yuvraj"
    ],
    "homepage": "https://github.com/test/yuvraj",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yuvraj singh",
            "email": "yuvrajsingh@exotel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Test\\Yuvraj\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Yuvraj\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Test\\Yuvraj\\YuvrajServiceProvider"
            ],
            "aliases": {
                "Yuvraj": "Test\\Yuvraj\\YuvrajFacade"
            }
        }
    }
}
